  "dependencies": {
    "tokenize-comment": "^3.0.1"
  },
  "devDependencies": {
    "@hapi/hapi": "^20.1.3",
    "@hapi/inert": "^6.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0"
  }
